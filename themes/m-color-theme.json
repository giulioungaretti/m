{
    "name": "m",
    "type": "light",
    "colors": {
        /* error squiggles */
        // "foreground": "#db029a",
        "editorError.foreground": "#92253d",
        /* warning squggles */
        "editorWarning.foreground": "#ffc400",
        /* info squiggles */
        "editorInfo.foreground": "#009959",
        "editor.background": "#fafafa",
        "editorLineNumber.foreground": "#C7CCD1",
        "editorLineNumber.activeForeground": "#000",
        "activityBarBadge.background": "#007acc",
        "sideBar.background": "#fafafa",
        "titleBar.activeBackground": "#fafafa",
        "titleBar.inactiveBackground": "#fafafa",
        "sideBarSectionHeader.background": "#fafafa",
        "activityBar.background": "#fafafa",
        "activityBar.foreground": "#C7CCD1",
        "tab.activeBackground": "#fafafa",
        "tab.inactiveBackground": "#fafafa",
        "tab.border": "#fafafa",
        "tab.activeBorder": "#000",
        "editorGroupHeader.tabsBackground": "#fafafa",
        "menu.background": "#fafafa",
        "panel.background": "#fafafa",
        "panel.border": "#fafafa",
        "scrollbarSlider.background": "#C7CCD1",
        "statusBar.background": "#fafafa",
        "statusBarItem.remoteBackground": "#fafafa",
        "statusBar.foreground": "#222",
        "statusBar.noFolderBackground": "#fafafa",
        "statusBar.noFolderForeground": "#C7CCD1",
        "statusBar.debuggingBackground": "#fafafa",
        "statusBar.debuggingForeground": "#C795AE",
        "statusBarItem.remoteForeground": "#222"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#838383",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#838383",
                "fontStyle": ""
            }
        },
        {
            "name": "interpolation",
            "scope": "keyword.format,meta.template.expression.ts, meta.interpolation, meta.fstring",
            "settings": {
                "foreground": "#000",
            }
        },
        {
            "name": "gitplus",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#009959",
                "fontStyle": "bold"
            }
        },
        {
            "name": "gitminus",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#92253d",
                "fontStyle": "bold"
            }
        },
        {
            "name": "gitchange",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#ffc400",
                "fontStyle": "bold"
            }
        },
    ]
}